  },
  "homepage": "./",
  "author": {
    "name": "Abhishek Vasvelia",
    "email": "abhivasvelia3460@gmail.com"
  },
  "build": {
    "appId": "com.app.hellopwa",
    "productName": "Hello PWA",
    "files": [
      "build/**/*",
      "electron/main.js",
      "package.json"
    ],
    "extends": null,
    "mac": {
      "type": "distribution"
    }
  }
